{
  "name": "@nezuchan/scheduled-tasks",
  "version": "1.2.0",
  "description": "A Standalone scheduled tasks service for microserviced \"Discord\" bot.",
  "license": "GPL-3.0",
  "author": "KagChi",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && swc src -d dist",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --fix --ext ts",
    "start": "npm run build && node -r dotenv/config dist/index.js"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "@hazmi35/eslint-config/typescript"
    ],
    "rules": {}
  },
  "dependencies": {
    "@nezuchan/cordis-brokers": "^2.0.0",
    "@sapphire/pieces": "^3.6.1",
    "@sapphire/result": "^2.6.0",
    "@sapphire/utilities": "^3.11.0",
    "bull": "^4.10.4",
    "dotenv": "^16.0.3",
    "pino": "^8.11.0",
    "pino-loki": "^2.0.4",
    "pino-pretty": "^10.0.0"
  },
  "devDependencies": {
    "@hazmi35/eslint-config": "^9.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.49",
    "@types/amqplib": "^0.10.1",
    "@types/bull": "^3.15.9",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "rimraf": "^4.4.1",
    "typescript": "^5.0.4"
  }
}
