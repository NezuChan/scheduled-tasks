{
  "name": "@nezuchan/scheduled-tasks",
  "version": "1.0.2",
  "description": "A Standalone scheduled tasks service for microserviced \"Discord\" bot.",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node -r dotenv/config dist/index.js",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --fix --ext ts",
    "build": "rimraf dist && swc src -d dist"
  },
  "type": "module",
  "author": "KagChi",
  "license": "GPL-3.0",
  "devDependencies": {
    "@hazmi35/eslint-config": "^8.4.2",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.223",
    "@types/amqplib": "^0.8.2",
    "@types/bull": "^3.15.9",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "@hazmi35/eslint-config/typescript"
    ],
    "rules": {}
  },
  "dependencies": {
    "@nezuchan/cordis-brokers": "^1.5.6",
    "@sapphire/pieces": "^3.5.1",
    "@sapphire/result": "^2.4.0",
    "@sapphire/utilities": "^3.9.2",
    "bull": "^4.8.5",
    "dotenv": "^16.0.1",
    "pino": "^8.3.1",
    "pino-pretty": "^8.1.0"
  }
}
